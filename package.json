{
  "name": "bonfhir-hackaton",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NEXTAUTH_URL=http://localhost:3000 next dev",
    "build": "next build",
    "start": "next start",
    "check": "prettier --check ./src && next lint && tsc --noEmit",
    "format": "prettier --write ./src",
    "register-subscriptions": "curl -i --request POST 'http://localhost:3000/api/fhir/subscriptions/register' --header 'X-Subscription-Auth: secret'",
    "prepare": "husky install"
  },
  "homepage": "TBD",
  "bugs": {
    "url": "https://github.com/AlleyCorpNord/bonfhir-hackaton/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlleyCorpNord/bonfhir-hackaton"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "dependencies": {
    "@bonfhir/core": "^2.16.1",
    "@bonfhir/mantine": "^3.1.1",
    "@bonfhir/next": "^2.4.0",
    "@bonfhir/query": "^2.5.0",
    "@bonfhir/react": "^2.8.0",
    "@bonfhir/subscriptions": "^2.3.0",
    "@mantine/core": "^7.2.2",
    "@mantine/dates": "^7.2.2",
    "@mantine/form": "^7.2.2",
    "@mantine/hooks": "^7.2.2",
    "@mantine/modals": "^7.2.2",
    "@mantine/notifications": "^7.2.2",
    "@mantine/tiptap": "^7.2.2",
    "@tabler/icons-react": "^2.41.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.3",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.11.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.17.1"
  }
}
